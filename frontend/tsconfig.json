{
    "files": [],
    "references": [
        {
            "path": "./tsconfig.app.json"
        },
        {
            "path": "./tsconfig.node.json"
        }
    ],
    "compilerOptions": {
        "baseUrl": ".",

        "paths": {
            "app/*": ["./src/app/*"],
            "css/*": ["./src/css/*"],
            "entities/*": ["./src/entities/*"],
            "features/*": ["./src/features/*"],
            "pages/*": ["./src/pages/*"],
            "shared/*": ["./src/shared/*"],
            "widgets/*": ["./src/widgets/*"],
            "assets/*": ["./src/assets/*"],
            "components/*": ["./src/components/*"],
            "ymaps3": ["./node_modules/@yandex/ymaps3-types"]
        },

        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@yandex/ymaps3-types"
        ]
    }
}
